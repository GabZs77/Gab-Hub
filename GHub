local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/RedzLibV5/main/Source.Lua"))()

local Window = redzlib:MakeWindow({
  Title = "üï∑Ô∏èGab HUBüï∑Ô∏è",
  SubTitle = "Feito por Brasileiro",
  SaveFolder = "Redz Config"
})

--Tabs
local Tab0 = Window:MakeTab({"‚ùÑÔ∏èGab Hub","cool"})
local Tab1 = Window:MakeTab({"üôÖüèºJogador", "cool"})
local Tab2 = Window:MakeTab({"üõ†Ô∏èAdmin", "cool"})
local Tab3 = Window:MakeTab({"üè°Brookhaven", "cool"})
local Tab4 = Window:MakeTab({"‚ú®Universal Scripts", "cool"})

--Buttons
Tab0:AddButton({"Youtube do developer", function()
setclipboard("https://m.youtube.com/@GhZinn077")
end})
Tab1:AddButton({"Vida", function()
